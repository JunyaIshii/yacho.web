{
    "compilerOptions": {
        "jsx": "react",
        "lib": ["es2023", "DOM", "DOM.Iterable"],
        "module": "ESNext",
        "target": "es2022",

        "baseUrl": "./",
        // "paths": {
        //     "@frontend/*": ["resources/ts/*"]
        // },

        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "moduleResolution": "Bundler"
    }
}
